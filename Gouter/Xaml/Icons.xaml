    <SolidColorBrush x:Key="TaskBarButton.Brush" Color="#FFFFFF" />
    <Pen x:Key="TaskBarButton.Border" Thickness="1" Brush="#666666" />

    <!-- 前トラックアイコン -->
    <DrawingImage x:Key="Images.PreviousTrack">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <GeometryDrawing
                    Brush="{StaticResource TaskBarButton.Brush}"
                    Pen="{StaticResource TaskBarButton.Border}"
                    Geometry="{StaticResource Icons.FastRewind}" />
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

    <!-- 再生アイコン -->
    <DrawingImage x:Key="Images.Play">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <GeometryDrawing
                    Brush="{StaticResource TaskBarButton.Brush}"
                    Pen="{StaticResource TaskBarButton.Border}"
                    Geometry="{StaticResource Icons.PlayArrow}" />
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

    <!-- 一時停止アイコン -->
    <DrawingImage x:Key="Images.Pause">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <GeometryDrawing
                    Brush="{StaticResource TaskBarButton.Brush}"
                    Pen="{StaticResource TaskBarButton.Border}"
                    Geometry="{StaticResource Icons.Pause}" />
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

    <!-- 次トラックアイコン -->
    <DrawingImage x:Key="Images.NextTrack">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <GeometryDrawing
                    Brush="{StaticResource TaskBarButton.Brush}"
                    Pen="{StaticResource TaskBarButton.Border}"
                    Geometry="{StaticResource Icons.FastForward}" />
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>
    
</ResourceDictionary>
